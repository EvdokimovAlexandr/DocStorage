<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="DocStorage" namespace="DocStorage.Models">
  <class name="Doc" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="DocId" type="int">
      <generator class="native" />
    </id>
    <property name="Author"/>
    <property name="Title" />
    <property name="DateOfCreation" type="datetime2"/>
    <property name="DocType"/>
    <property name="RefFile"/>
  </class>
</hibernate-mapping>